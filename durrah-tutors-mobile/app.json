{
  "expo": {
    "name": "Durrah for Tutors",
    "slug": "durrah-tutors-mobile",
    "version": "0.1.0",
    "description": "Professional exam management for tutors on the go",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
      "primaryColor": "#6366f1",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.durrah.tutors",
      "buildNumber": "1"
    },
    "android": {
        "package": "com.durrah.tutors",
        "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
